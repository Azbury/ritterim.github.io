.tags
    margin-top rem(30px)
    a
        font-size rem(14px)
        color blue
        display inline-block
        border 1px solid cadetblue
        border-radius rem(3px)
        padding rem(4px) rem(10px)
        margin-right rem(2px)
        margin-bottom rem(8px)
        text-decoration none
        &:hover
            color white
            background cadetblue

.img-rounded
    border-radius 50%
